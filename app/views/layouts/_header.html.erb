        </div><!-- /.navbar -->
      </div> <!-- /.container -->
</div>
<script type="text/javascript">
$(document).ready(function (){
	$('#faddress').show();
  $('#about').hide();
  $('#service').hide();
  $('#contact').hide();
  
  $('#roll1').click(function() {
      $('#faddress').show();
    	$('#about').show();
      $('#service').hide();
      $('#contact').hide();
		  $('html, body').animate({
    		scrollTop: $('#about').offset().top - $('html, body').offset().top + $('html, body').scrollTop()
		  }, 1500);
  });
  $('#roll2').click(function() {
    	$('#faddress').show();
      $('#about').hide();
      $('#service').show();
      $('#contact').hide();
		  $('html, body').animate({
    		scrollTop: $('#service').offset().top - $('html, body').offset().top + $('html, body').scrollTop()
		  }, 1500);
  });
  $('#roll3').click(function() {
    	$('#faddress').hide();
      $('#about').hide();
      $('#service').hide();
      $('#contact').show();
		  $('html, body').animate({
    		scrollTop: $('#contact').offset().top - $('html, body').offset().top + $('html, body').scrollTop()
		  }, 1500);
  });
  	
});
</script>